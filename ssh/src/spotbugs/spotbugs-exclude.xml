<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,CT_CONSTRUCTOR_THROW,DP_CREATE_CLASSLOADER_INSIDE_DO_PRIVILEGED"/>
    </Match>
    <Match>
        <Class name="org.dellroad.jct.ssh.AbstractCommand"/>
        <Field name="locale"/>
        <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD"/>
    </Match>
</FindBugsFilter>
